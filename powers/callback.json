{
	"type": "origins:action_on_callback",
	"hidden": true,
	"entity_action_respawned": {
		"type": "origins:and",
		"actions": [
			{
				"type": "origins:change_resource",
				"resource": "fused:ef_duration_dura",
				"change": -33
			},
			{
				"type": "origins:change_resource",
				"resource": "fused:ef_cooldown_cd",
				"change": -80
			},
			{
				"type": "origins:change_resource",
				"resource": "fused:heal_cooldown",
				"change": -360
			},
			{
				"type": "origins:change_resource",
				"resource": "fused:ef_cooldown_fall",
				"change": -1
			},
			{
				"type": "origins:change_resource",
				"resource": "fused:repossession_cost",
				"change": 0,
				"operation": "set"
			},
			{
				"type": "origins:change_resource",
				"resource": "fused:repossession_cooldown",
				"change": -2400
			},
			{
				"type": "origins:change_resource",
				"resource": "fused:animated_damage_over_time",
				"change": 0,
				"operation": "set"
			}
		]
	},
	"entity_action_chosen": {
		"type": "origins:and",
		"actions": [
			{
				"type": "origins:feed",
				"food": 20,
				"saturation": 20
			},
			{
				"type": "origins:execute_command",
				"command": "function fused:init"
			}
		]
	},
	"entity_action_added": {
		"type": "origins:if_else",
		"condition": {
			"type": "origins:command",
			"command": "execute if entity @s[tag=fusedStar]",
			"comparison": "==",
			"compare_to": 1
		},
		"if_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "tag @s remove fusedStar"
				},
				{
					"type": "origins:execute_command",
					"command": "give @s nether_star"
				}
			]
		}
	},
	"entity_action_lost": {
		"type": "origins:and",
		"actions": [
			{
				"type": "origins:execute_command",
				"command": "tag @s remove fusedFused"
			},
			{
				"type": "origins:execute_command",
				"command": "power revoke @s fused:repossession_countdown fused:repossession"
			},
			{
				"type": "origins:revoke_power",
				"power": "fused:armor_25",
				"source": "hit_7.5"
			},
			{
				"type": "origins:revoke_power",
				"power": "fused:armor_50",
				"source": "hit_15"
			}
		]
	}
}